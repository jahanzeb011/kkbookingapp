<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Kala Kothi Lawn Booking Application</title>
    <link rel="stylesheet" href="/css/styles.css">
    <style>
        .booking-item {
            padding: 10px;
            border: 1px solid #ddd;
            margin-bottom: 10px;
            position: relative;
        }
        .booking-item.checked {
            background-color: #d4edda; /* Light green background */
        }
        .checkbox-container {
            position: absolute;
            top: 10px;
            right: 10px;
        }
        .checkbox-container input[type="checkbox"] {
            margin: 0;
        }

          /* Basic styles for the modal */
          #pinModal {
            display: none;
            position: fixed;
            z-index: 1000;
            left: 0;
            top: 0;
            width: 100%;
            height: 100%;
            background-color: rgba(0, 0, 0, 0.8);
            justify-content: center;
            align-items: center;
        }
        #pinModalContent {
            background-color: white;
            padding: 20px;
            border-radius: 8px;
            text-align: center;
        }
        #pinModal input[type="password"] {
            padding: 10px;
            font-size: 16px;
            margin-top: 10px;
        }
        #pinModal button {
            padding: 10px 20px;
            font-size: 16px;
            margin-top: 10px;
            cursor: pointer;
        }
    

        @media (max-width: 768px) {
    .checkbox-container {
        top: 5px;
        left: 5px;
    }

    .payment-label {
        font-size: 12px; /* Smaller text for smaller screens */
    }

    .booking-item {
        padding-top: 40px; /* Add padding to make space for the checkbox */
        position: relative;
    }
}

        
    </style>
</head>
<body>

      <!-- Modal for PIN entry -->
      <div id="pinModal">
        <div id="pinModalContent">
            <h2>Enter PIN to Access</h2>
            <input type="password" id="pinInput" placeholder="Enter PIN">
            <button onclick="checkPin()">Submit</button>
            <p id="pinError" style="color: red; display: none;">Incorrect PIN. Please try again.</p>
        </div>
    </div>

    <h1>Kala Kothi Lawn Booking Application</h1>
    <form id="initialBookingForm">
        <label for="lawn">Choose Lawn:</label>
        <select name="lawn" id="lawn" required>
            <option value="Lawn 1">Lawn 1</option>
            <option value="Lawn 2">Lawn 2</option>
        </select>
        <label for="date">Choose Date:</label>
        <input type="date" name="date" id="date" required>
        <button type="button" id="openModal">Book Now</button>
    </form>

    <div id="bookingModal" class="modal">
        <div class="modal-content">
            <span class="close">&times;</span>
            <h2>Complete Your Booking</h2>
            <form id="detailedBookingForm">
                <input type="hidden" id="selectedLawn" name="lawn">
                <input type="hidden" id="selectedDate" name="date">
                <label for="name">Name:</label>
                <input type="text" id="name" name="name" required>
                <label for="phone">Phone:</label>
                <input type="tel" id="phone" name="phone" required>
                <label for="email">Email:</label>
                <input type="email" id="email" name="email">
                <label for="address">Address:</label>
                <input type="text" id="address" name="address" required>
                <label for="totalAmount">Total Amount:</label>
                <input type="number" id="totalAmount" name="totalAmount" required>
                <label for="discountedAmount">Discounted Amount:</label>
                <input type="number" id="discountedAmount" name="discountedAmount">
                <label for="advancePaid">Advance Paid:</label>
                <input type="number" id="advancePaid" name="advancePaid" required>
                <label for="duePayment">Due Payment:</label>
                <input type="number" id="duePayment" name="duePayment" required>
                <label for="additionalServices">Additional Services:</label>
                <textarea id="additionalServices" name="additionalServices" rows="4"></textarea>

                <button type="submit">Submit Booking</button>
            </form>
        </div>
    </div>

    <!-- Button to toggle bookings list view -->
    <button id="toggleBookingsButton">View Bookings</button>

    <!-- Bookings List (hidden by default) -->
    <div id="bookingsList" style="display: none;">
        <h2>All Bookings</h2>
        <ul id="bookingList">
            <% bookings.forEach(booking => { %>
                <li id="booking-<%= booking._id %>" class="booking-item">
                    <div class="checkbox-container">
                        <label>
                            <input type="checkbox" class="paymentClearedCheckbox">
                            All Payments Cleared
                        </label>
                    </div>
                    <strong>Lawn:</strong> <%= booking.lawn %> <br>
                    <strong>Date:</strong> <%= booking.displayDate %> <br>
                    <strong>Name:</strong> <%= booking.name %> <br>
                    <strong>Phone:</strong> <%= booking.phone %> <br>
                    <strong>Email:</strong> <%= booking.email %> <br>
                    <strong>Address:</strong> <%= booking.address %> <br>
                    <strong>Total Amount:</strong> <%= booking.totalAmount %> <br>
                    <strong>Discounted Amount:</strong> <%= booking.discountedAmount %> <br>
                    <strong>Advance Paid:</strong> <%= booking.advancePaid %> <br>
                    <strong>Due Payment:</strong> <%= booking.duePayment %> <br>
                    <strong>Additional Services:</strong> <%= booking.additionalServices %> <br>
                    <button class="cancelButton" data-id="<%= booking._id %>">Cancel Booking</button>
                    <button class="downloadButton" data-id="<%= booking._id %>">Download PDF</button>
                </li>
            <% }) %>
        </ul>
    </div>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/jspdf/2.5.1/jspdf.umd.min.js"></script>
    <script>
         document.getElementById('openModal').addEventListener('click', function() {
            const lawn = document.getElementById('lawn').value;
            const date = document.getElementById('date').value;

            document.getElementById('selectedLawn').value = lawn;
            document.getElementById('selectedDate').value = date;

            document.getElementById('bookingModal').style.display = 'block';
        });

        document.querySelector('.close').addEventListener('click', function() {
            document.getElementById('bookingModal').style.display = 'none';
            document.getElementById('detailedBookingForm').reset();  // Clear the form
        });

        document.getElementById('detailedBookingForm').addEventListener('submit', async function(event) {
            event.preventDefault();

            const dateInput = document.getElementById('selectedDate').value;

            // Check if the date is selected
            if (!dateInput) {
                alert('Please select a date.');
                return;
            }

            const selectedDate = new Date(dateInput);
            const today = new Date();
            today.setHours(0, 0, 0, 0); // Normalize time to midnight to avoid issues

            // Prevent booking for past dates
            if (selectedDate < today) {
                alert('Booking for past dates is not allowed.');
                return;
            }

            const formData = new FormData(event.target);
            const data = Object.fromEntries(formData);

            const response = await fetch('/book', {
                method: 'POST',
                headers: { 'Content-Type': 'application/json' },
                body: JSON.stringify(data)
            });

            if (response.ok) {
                alert('Booking successful!');
                event.target.reset();  // Clear the form
                window.location.reload();
            } else {
                const result = await response.json();
                alert(result.message || 'Booking failed.');
            }
        });

        // Toggle the visibility of the bookings list
        document.getElementById('toggleBookingsButton').addEventListener('click', function() {
            const bookingsList = document.getElementById('bookingsList');
            const button = document.getElementById('toggleBookingsButton');

            if (bookingsList.style.display === 'none' || bookingsList.style.display === '') {
                bookingsList.style.display = 'block';
                button.textContent = 'Hide Bookings';
            } else {
                bookingsList.style.display = 'none';
                button.textContent = 'View Bookings';
            }
        });

        // Handle cancel booking button click
        document.addEventListener('click', async function(event) {
            if (event.target.classList.contains('cancelButton')) {
                const bookingId = event.target.getAttribute('data-id');

                if (confirm('Are you sure you want to cancel this booking?')) {
                    try {
                        const response = await fetch(`/cancel/${bookingId}`, { method: 'DELETE' });
                        
                        if (response.ok) {
                            alert('Booking canceled successfully.');
                            // Remove the booking from the list
                            event.target.closest('li').remove();
                        } else {
                            const result = await response.json();
                            alert(result.message || 'Cancellation failed.');
                        }
                    } catch (error) {
                        alert('An error occurred.');
                    }
                }
            }
        });

        // Handle download PDF button click
        document.addEventListener('click', async function(event) {
            if (event.target.classList.contains('downloadButton')) {
                const bookingId = event.target.getAttribute('data-id');
                const bookingElement = event.target.closest('li');
                
                // Create a copy of the <li> content
                const content = bookingElement.cloneNode(true);

                // Remove buttons and checkboxes
                const buttons = content.querySelectorAll('button');
                buttons.forEach(button => button.remove());
                
                const checkboxes = content.querySelectorAll('input[type="checkbox"]');
                checkboxes.forEach(checkbox => checkbox.remove());

                const labels = content.querySelectorAll('label');
                labels.forEach(label => {
                    if (label.textContent.includes('All Payments Cleared')) {
                        label.remove();
                    }
                });

                
                
                // Generate PDF using jsPDF
                const { jsPDF } = window.jspdf;
                const doc = new jsPDF();

                 // Add Base64 Image
        const base64Image = 'data:image/png;base64,';  // Replace with your actual base64 string
        doc.addImage(base64Image, 'PNG', 150, 5, 40, 40);  // Adjust the position and size as needed

                

        // Set up the PDF document
doc.setFont('helvetica'); // Choose a formal font
doc.setLineWidth(0.5);

// Add a border for the receipt
doc.rect(5, 5, 200, 287); // A4 page size, with some margins

// Add Heading at the top left corner
doc.setFontSize(20);
doc.setFont('helvetica', 'bold');
doc.text('Kala Kothi Wedding Lawn', 10, 20);

// Add Contact Information below the Heading
doc.setFontSize(12);
doc.setFont('helvetica', 'normal');
doc.text('Contact: +919044844950', 10, 30);
doc.text('Email: kalakothi2018@gmail.com', 10, 35);
doc.text('Instagram: @kalakothiweddinglawns', 10, 40);
doc.text('Facebook: @kalakothiweddinglawns', 10, 45);
doc.text('Address: Kudiya Ghat Bandha Road, Near Shani Mandir, Hussainabad Chowk, Lucknow', 10, 50);

// Add a horizontal line to separate the header
doc.setLineWidth(0.5);
doc.line(10, 52, 200, 52);

// Add Sub Heading
doc.setFontSize(18);
doc.setFont('helvetica', 'underline');
doc.text('Booking Receipt', 105, 65, null, null, 'center');

// Add a box for the content
doc.setLineWidth(0.5);
doc.rect(10, 70, 190, 160); // A box for the content

// Add content inside the box
doc.setFontSize(16);
doc.setFont('helvetica', 'underline');
let yPosition = 50; // Starting position for the content inside the box

// Assuming content.innerText contains multiple lines, each line needs spacing
const contentLines = content.innerText.split('\n');
contentLines.forEach((line) => {
    doc.text(line, 12, yPosition); // Adjust text inside the box
    yPosition += 10; // Add spacing between lines
});

// Add additional text at the bottom of the box with spacing
doc.setFontSize(12);
doc.setTextColor(100); // Gray color for terms and conditions
yPosition += 10; // Add some extra space before the terms and conditions

const additionalText = [
    '* Booking Amount non-refundable.',
    '* Balance Payment due a week before the event.',
    '* Lawn available from 8 am to next day 8 am.',
    '* Govt taxes & levies extra as applicable.',
    '* All Disputes are subject to Lucknow Jurisdiction.'
];

additionalText.forEach((line) => {
    doc.text(line, 15, yPosition+20); // Adjust text inside the box
    yPosition += 5; // Add spacing between terms
});

// Add a customer signature column at the bottom right corner
doc.setFontSize(14);
doc.setFont('helvetica', 'normal');
doc.text('Customer Signature:', 150, 250); // Position the signature line
doc.line(150, 252, 200, 252); // Add the line for the signature

// Add a footer with page number
doc.setFontSize(10);
doc.setTextColor(140);

// Save the PDF
doc.save(`Booking_${bookingId}.pdf`);



            }
        });

        // Handle checkbox state change
        document.addEventListener('DOMContentLoaded', function () {
    // Handle checkbox state change
    document.querySelectorAll('.paymentClearedCheckbox').forEach(checkbox => {
        const bookingId = checkbox.closest('li').id;

        // Load the checkbox state from local storage
        const isChecked = localStorage.getItem(`paymentCleared_${bookingId}`) === 'true';
        checkbox.checked = isChecked;

        // Update the visual state
        if (isChecked) {
            checkbox.closest('li').classList.add('checked');
        } else {
            checkbox.closest('li').classList.remove('checked');
        }

        // Handle change event
        checkbox.addEventListener('change', function () {
            const isChecked = checkbox.checked;

            // Store the checkbox state in local storage
            localStorage.setItem(`paymentCleared_${bookingId}`, isChecked);

            // Update the visual state
            if (isChecked) {
                checkbox.closest('li').classList.add('checked');
            } else {
                checkbox.closest('li').classList.remove('checked');
            }
        });
    });
});

const correctPin = '1973';

function checkPin() {
    const userPin = document.getElementById('pinInput').value;
    if (userPin === correctPin) {
        document.getElementById('pinModal').style.display = 'none';
        document.getElementById('content').style.display = 'block';
    } else {
        document.getElementById('pinError').style.display = 'block';
    }
}

window.onload = function() {
    document.getElementById('pinModal').style.display = 'flex';
};
    </script>
</body>
</html>
